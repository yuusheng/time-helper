{
  "manifest_version": 3,
  "name": "time helper",
  "version": "0.0.1",
  "description": "extension can help you to statistics your time on website",
  "action": {
    "default_icon": {
      "16": "/images/icon.png",
      "32": "/images/icon.png",
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    },
    "default_title": "time helper",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "/images/icon.png",
    "32": "/images/icon.png",
    "48": "/images/icon.png",
    "128": "/images/icon.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"]
}
